{
  "version": 2,
  "builds": [
    {
      "src": "server/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ]
}